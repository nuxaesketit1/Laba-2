#include <iostream>
#include <string>
#include <cmath>
using namespace std;
int main()
{
	double x1, x2, y1, y2;

	cout << "Enter the coordinates of the 1st point : " << endl;
	cin >> x1 >> y1;

	cout << "Enter the coordinates of the 2nd point : " << endl;
	cin >> x2 >> y2;

	cout << "The distance between two points is :" << sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2)) << endl;

	return 0;

}
